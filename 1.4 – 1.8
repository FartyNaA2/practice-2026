class Animal:
    def speak(self):
        pass

class Dog(Animal):
    def __init__(self, name):
        self.__name = name 
    
    def get_name(self):
        return self.__name

    def speak(self):
        return "Woof!"

class Cat(Animal):
    def speak(self):
        return "Meow!"

animals = [Dog("Rex"), Cat()]
for a in animals:
    print(a.speak())
